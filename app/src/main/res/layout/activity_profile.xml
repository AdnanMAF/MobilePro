<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profileVM" type="com.adnansapplication.app.modules.profile.data.viewmodel.ProfileVM"/>
    </data>
    <FrameLayout android:id="@+id/frameProfile"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="top|start"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_25pxv"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="@dimen/_6pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft_bluegray_300"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_bluegray_300"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtProfile"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_21pxh"
                        android:layout_weight="0.9"
                        android:layout_width="0dp"
                        android:letterSpacing="0.03"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtProfile}"
                        style="@style/txtPoppinsbold16" tools:text="@string/lbl_profile"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowprofilepicture"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_52pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageProfilePicture"
                    android:layout_height="@dimen/_72pxh"
                    android:layout_width="@dimen/_72pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_profilepicture}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_profilepicture"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnname"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_weight="0.58"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtName"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.03"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtName}"
                        style="@style/txtPoppinsbold14_1" tools:text="@string/lbl_dominic_ovo"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtUsername"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.03"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtUsername}"
                        style="@style/txtPoppinsregular12" tools:text="@string/lbl_dominic_ovo2"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearProfiledetail"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_32pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700">
                <LinearLayout android:id="@+id/linearRowlaptop"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_weight="0.57"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageLaptop"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_width="@dimen/_12pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_laptop"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_laptop"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGender"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.8"
                        android:layout_width="0dp"
                        android:letterSpacing="0.03"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtGender}"
                        style="@style/txtPoppinsbold12" tools:text="@string/lbl_gender"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowgenderOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_184pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_weight="0.43"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGenderOne"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.83"
                        android:layout_width="0dp"
                        android:letterSpacing="0.03"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtGenderOne}"
                        style="@style/txtPoppinsregular12" tools:text="@string/lbl_male"/>
                    <ImageView android:id="@+id/imageArrowright"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_6pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright_bluegray_300"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_bluegray_300"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearProfiledetail1"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700">
                <LinearLayout android:id="@+id/linearRowcalendar"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_weight="0.48"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCalendar"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_calendar"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_calendar"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtBirthday"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.76"
                        android:layout_width="0dp"
                        android:letterSpacing="0.03"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtBirthday}"
                        style="@style/txtPoppinsbold12" tools:text="@string/lbl_birthday"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowdate"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_137pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_weight="0.52"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDate"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.92"
                        android:layout_width="0dp"
                        android:letterSpacing="0.03"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtDate}"
                        style="@style/txtPoppinsregular12" tools:text="@string/lbl_12_12_2000"/>
                    <ImageView android:id="@+id/imageArrowrightOne"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_6pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright_bluegray_300"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_bluegray_300"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearProfiledetail2"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700">
                <LinearLayout android:id="@+id/linearRowarrowdown"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_weight="0.31"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowdown"
                        android:layout_height="@dimen/_15pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowdown_light_blue_a200_15x20"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowdown_light_blue_a200_15x20"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEmail"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_weight="0.65"
                        android:layout_width="0dp"
                        android:letterSpacing="0.03"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtEmail}"
                        style="@style/txtPoppinsbold12" tools:text="@string/lbl_email"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowemailOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_88pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_weight="0.69"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEmailOne"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.96"
                        android:layout_width="0dp"
                        android:letterSpacing="0.03"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtEmailOne}"
                        style="@style/txtPoppinsregular12" tools:text="@string/msg_rex4dom_gmail_com"/>
                    <ImageView android:id="@+id/imageArrowrightTwo"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_6pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright_bluegray_300"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_bluegray_300"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearProfiledetail3"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700">
                <LinearLayout android:id="@+id/linearRowtelevision"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_weight="0.51"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageTelevision"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_11pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_television_light_blue_a200_13x18"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_television_light_blue_a200_13x18"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPhoneNumber"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_weight="0.9"
                        android:layout_width="0dp"
                        android:letterSpacing="0.03"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtPhoneNumber}"
                        style="@style/txtPoppinsbold12" tools:text="@string/lbl_phone_number"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowmobilenumberv"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_69pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_weight="0.49"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMobilenumberv"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.94"
                        android:layout_width="0dp"
                        android:letterSpacing="0.03"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtMobilenumberv}"
                        style="@style/txtPoppinsregular12" tools:text="@string/lbl_307_555_0133"/>
                    <ImageView android:id="@+id/imageArrowrightThree"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_6pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright_bluegray_300"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_bluegray_300"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearProfiledetail4"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_A700">
                <LinearLayout android:id="@+id/linearRowlocation"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_weight="0.56"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageLocation"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_location_light_blue_a200"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_location_light_blue_a200"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtChangePassword"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="0.86"
                        android:layout_width="0dp"
                        android:letterSpacing="0.03"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtChangePassword}"
                        style="@style/txtPoppinsbold12" tools:text="@string/lbl_change_password"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowseven"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_52pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_weight="0.44"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSeven"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.94"
                        android:layout_width="0dp"
                        android:letterSpacing="0.03"
                        android:singleLine="true"
                        android:text="@{profileVM.profileModel.txtSeven}"
                        style="@style/txtPoppinsregular12" tools:text="@string/msg"/>
                    <ImageView android:id="@+id/imageArrowrightFour"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_6pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright_bluegray_300"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_bluegray_300"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
