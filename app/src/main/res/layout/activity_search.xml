<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="searchVM" type="com.adnansapplication.app.modules.search.data.viewmodel.SearchVM"/>
    </data>
    <LinearLayout android:gravity="start" android:id="@+id/linearSearch"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowclose"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearSearch1"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.96"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStylecornerRadius_2">
                    <LinearLayout android:id="@+id/linearRowrewind"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_weight="0.91"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageRewind"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_rewind"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rewind"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtNikeAirMax"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_9pxh"
                            android:layout_weight="0.85"
                            android:layout_width="0dp"
                            android:letterSpacing="0.03"
                            android:singleLine="true"
                            android:text="@{searchVM.searchModel.txtNikeAirMax}"
                            style="@style/txtPoppinsbold12_4" tools:text="@string/lbl_nike_air_max"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageClose"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginEnd="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_137pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="@dimen/_10pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_close"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageMenu"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginStart="@dimen/_33pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_menu"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_menu"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <View android:background="@color/blue_50"
            android:id="@+id/lineLineThirtyNine"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_6pxh"
            android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_369pxh"/>
        <TextView android:ellipsize="end" android:id="@+id/txtLanguage"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_26pxv"
            android:layout_width="wrap_content"
            android:letterSpacing="0.03" android:singleLine="true"
            android:text="@{searchVM.searchModel.txtLanguage}"
            style="@style/txtPoppinsregular12" tools:text="@string/msg_nike_air_max_2703"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtNikeAirVaporm"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_37pxv"
            android:layout_width="wrap_content"
            android:letterSpacing="0.03" android:singleLine="true"
            android:text="@{searchVM.searchModel.txtNikeAirVaporm}"
            style="@style/txtPoppinsregular12" tools:text="@string/msg_nike_air_vapormax"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtLanguageOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_34pxv"
            android:layout_width="wrap_content"
            android:letterSpacing="0.03" android:singleLine="true"
            android:text="@{searchVM.searchModel.txtLanguageOne}"
            style="@style/txtPoppinsregular12" tools:text="@string/msg_nike_air_max_2703"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtNikeAirMaxTwentySeven"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_36pxv"
            android:layout_width="wrap_content"
            android:letterSpacing="0.03" android:singleLine="true"
            android:text="@{searchVM.searchModel.txtNikeAirMaxTwentySeven}"
            style="@style/txtPoppinsregular12" tools:text="@string/msg_nike_air_max_2704"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtNikeAirVaporMOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_37pxv"
            android:layout_width="wrap_content"
            android:letterSpacing="0.03" android:singleLine="true"
            android:text="@{searchVM.searchModel.txtNikeAirVaporMOne}"
            style="@style/txtPoppinsregular12" tools:text="@string/msg_nike_air_vapormax2"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtNikeAirMaxNinetySeven"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_35pxv"
            android:layout_width="wrap_content"
            android:letterSpacing="0.03" android:singleLine="true"
            android:text="@{searchVM.searchModel.txtNikeAirMaxNinetySeven}"
            style="@style/txtPoppinsregular12" tools:text="@string/msg_nike_air_max_97"/>
    </LinearLayout>
</layout>
